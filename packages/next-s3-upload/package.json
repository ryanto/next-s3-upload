  "repository": "github:ryanto/next-s3-upload",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "exports": {
    ".": {
